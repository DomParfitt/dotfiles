{
  "[css]": {
    "editor.suggest.insertMode": "replace",
    "editor.tabSize": 2
  },
  "[haskell]": {
    "editor.defaultFormatter": "MaxGabriel.brittany"
  },
  "[helm]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.tabSize": 2
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.tabSize": 2
  },
  "[latex]": {
    "editor.defaultFormatter": "nickfode.latex-formatter"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one",
    "editor.tabSize": 2
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "[rust]": {
    "editor.tabSize": 4
  },
  "[shellscript]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "breadcrumbs.enabled": true,
  "calva.paredit.defaultKeyMap": "strict",
  "calva.showCalvaSaysOnStart": false,
  "coverage-gutters.coverageReportFileName": "test-results/jest/**, .nyc_output/**, results/unit/**",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never"
  },
  "editor.detectIndentation": false,
  "editor.fontSize": 12,
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.largeFileOptimizations": false,
  "editor.minimap.enabled": false,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "emmet.showAbbreviationSuggestions": false,
  "emmet.showExpandedAbbreviation": "never",
  "evenBetterToml.semanticTokens": true,
  "extensions.ignoreRecommendations": false,
  "files.associations": {
    "*.cf": "yaml",
    "*.inc": "php",
    "*.module": "php",
    "*.vtc": "vcl"
  },
  "files.insertFinalNewline": true,
  "files.watcherExclude": {
    "**/.ammonite": true,
    "**/.bloop": true,
    "**/.metals": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "gitlens.advanced.messages": {
    "suppressIntegrationRequestTimedOutWarning": true
  },
  "gitlens.launchpad.indicator.polling.enabled": false,
  "go.alternateTools": {
    "customFormatter": "./bin/gosimports"
  },
  "go.buildTags": "linux",
  "go.delveConfig": {
    "showGlobalVariables": true,
    "showLog": true
  },
  "go.inlayHints.parameterNames": true,
  "go.inlayHints.rangeVariableTypes": true,
  "go.lintTool": "golangci-lint",
  "go.testFlags": [
    "--count=1"
  ],
  "go.testTags": "integration",
  "go.toolsManagement.autoUpdate": true,
  "gopls": {
    "build.buildFlags": [
      "-tags=build e2e integration prod"
    ],
    "ui.semanticTokens": true
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "latex-workshop.view.pdf.viewer": "tab",
  "markdown.extension.toc.levels": "2..6",
  "markdown.extension.toc.orderedList": true,
  "php.validate.executablePath": "/usr/bin/php",
  "prettier.requireConfig": true,
  "prettier.singleQuote": true,
  "python.defaultInterpreterPath": "/bin/python",
  "redhat.telemetry.enabled": false,
  "rust-analyzer.inlayHints.bindingModeHints.enable": true,
  "rust-analyzer.inlayHints.closingBraceHints.enable": false,
  "rust-analyzer.semanticHighlighting.operator.specialization.enable": true,
  "security.workspace.trust.enabled": false,
  "shellcheck.customArgs": [
    "-x"
  ],
  "shellcheck.disableVersionCheck": true,
  "shellcheck.enableQuickFix": true,
  "shellcheck.ignorePatterns": {
    "**/*.fish": true,
    "**/*.zsh": true
  },
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "literals",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.titleBarStyle": "custom",
  "workbench.colorCustomizations": {
    "editorInlayHint.background": "#282c34",
    "editorInlayHint.foreground": "#7f848e"
  },
  "workbench.colorTheme": "One Dark Pro",
  "workbench.editor.enablePreview": false,
  "workbench.startupEditor": "newUntitledFile",
  "xml.format.enabled": false,
  "yaml.format.enable": true,
  "yaml.schemas": {
    "Kubernetes": "*.yaml"
  },
  "yaml.schemaStore.enable": false,
  "yaml.validate": false
}
